{
  "version": 2,
  "builds": [
    {
      "src": "api_server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api_server.py"
    }
  ],
  "env": {
    "HF_TOKEN": "@hf_token"
  },
  "functions": {
    "api_server.py": {
      "maxDuration": 30
    }
  }
}